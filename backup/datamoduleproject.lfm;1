  object dataProjectForOrg: TOraQuery
    SQL.Strings = (
      'SELECT '
      ' proj.projectname'
      '   ,prod.PRODUCTCODE'
      '   ,prodV.VERSION'
      '   ,proj.*'
      ''
      '  FROM CORE_PROJECT proj'
      ''
      '   INNER JOIN  GLOBAL_PRODUCTVERSION prodV'
      '     ON proj.PRODUCTVERSION_ID = prodV.PRODUCTVERSION_ID'
      '   INNER JOIN GLOBAL_PRODUCT prod'
      '     ON prodV.PRODUCT_ID = prod.PRODUCT_ID'
      'ORDER BY proj.ISACTIVE DESC, ProductCode'
    )
    MasterSource = DM_Main.dsAccount
    MasterFields = 'ACCOUNT_ID'
    DetailFields = 'ACCOUNT_ID'
