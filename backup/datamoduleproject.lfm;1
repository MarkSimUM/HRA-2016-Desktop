      ''
      ')'
    )
    SQL.Strings = (
      'SELECT '
      '  CORE_PROJECT.* '
      '  ,prod.productCode'
      '  ,prodv.version'
      ''
      '  FROM CORE_PROJECT'
      '   '
      '   INNER JOIN GLOBAL_PRODUCTVERSION prodV'
      '     ON CORE_PROJECT.PRODUCTVERSION_ID = prodV.PRODUCTVERSION_ID'
      ''
      '   INNER JOIN GLOBAL_PRODUCT prod'
      '     ON prodV.product_ID = prod.product_ID'
      ''
      ' ORDER BY isActive DESC'
    )
