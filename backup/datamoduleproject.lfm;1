  object countCurrentProject: TOraQuery
    SQL.Strings = (
      'select count(*) AS CurrentCount'
      '  FROM CORE_PROJECT proj'
      '  INNER JOIN GLOBAL_PRODUCTVERSION  pver'
      '  ON proj.PRODUCTVERSION_ID = pver.PRODUCTVERSION_ID'
      '   AND pver.PRODUCT_ID = :PRODUCT_ID'
      '   '
      '   WHERE proj.ACCOUNT_ID = :ACCOUNT_ID'
    )
