    SQLInsert.Strings = (
      'INSERT INTO CORE_PROJECT'
      '  (PROJECT_ID, ACCOUNT_ID, LINKEDPROJECT_ID, PROJECTNAME, ISTEST, ISACTIVE, ISMULTIPRODUCT, ISMULTIORG, PHASE, UNIQUEIDFIELDNAME, NOTES, PRODUCTVERSION_ID, PROJECTGROUP_ID, EVALANALYSIS_FLG, LOCKSETTINGS_FLG, STATUS_ID, AUTOADDNEWORG_FLG, PROJECT_START_DATE, PROJECT_END_DATE, DELETE_FLG)'
      'VALUES'
      '  (:PROJECT_ID, :ACCOUNT_ID, :LINKEDPROJECT_ID, :PROJECTNAME, :ISTEST, :ISACTIVE, :ISMULTIPRODUCT, :ISMULTIORG, :PHASE, :UNIQUEIDFIELDNAME, :NOTES, :PRODUCTVERSION_ID, :PROJECTGROUP_ID, :EVALANALYSIS_FLG, :LOCKSETTINGS_FLG, :STATUS_ID, :AUTOADDNEWORG_FLG, :PROJECT_START_DATE, :PROJECT_END_DATE, :DELETE_FLG)'
    )
    SQLDelete.Strings = (
      'DELETE FROM CORE_PROJECT'
      'WHERE'
      '  PROJECT_ID = :Old_PROJECT_ID'
    )
    SQLUpdate.Strings = (
      'UPDATE CORE_PROJECT'
      'SET'
      '  PROJECT_ID = :PROJECT_ID, ACCOUNT_ID = :ACCOUNT_ID, LINKEDPROJECT_ID = :LINKEDPROJECT_ID, PROJECTNAME = :PROJECTNAME, ISTEST = :ISTEST, ISACTIVE = :ISACTIVE, ISMULTIPRODUCT = :ISMULTIPRODUCT, ISMULTIORG = :ISMULTIORG, PHASE = :PHASE, UNIQUEIDFIELDNAME = :UNIQUEIDFIELDNAME, NOTES = :NOTES, PRODUCTVERSION_ID = :PRODUCTVERSION_ID, PROJECTGROUP_ID = :PROJECTGROUP_ID, EVALANALYSIS_FLG = :EVALANALYSIS_FLG, LOCKSETTINGS_FLG = :LOCKSETTINGS_FLG, STATUS_ID = :STATUS_ID, AUTOADDNEWORG_FLG = :AUTOADDNEWORG_FLG, PROJECT_START_DATE = :PROJECT_START_DATE, PROJECT_END_DATE = :PROJECT_END_DATE, DELETE_FLG = :DELETE_FLG'
      'WHERE'
      '  PROJECT_ID = :Old_PROJECT_ID'
    )
    SQLLock.Strings = (
      'SELECT PROJECT_ID, ACCOUNT_ID, LINKEDPROJECT_ID, PROJECTNAME, ISTEST, ISACTIVE, ISMULTIPRODUCT, ISMULTIORG, PHASE, UNIQUEIDFIELDNAME, NOTES, PRODUCTVERSION_ID, PROJECTGROUP_ID, EVALANALYSIS_FLG, LOCKSETTINGS_FLG, STATUS_ID, AUTOADDNEWORG_FLG, PROJECT_START_DATE, PROJECT_END_DATE, DELETE_FLG FROM CORE_PROJECT'
      'WHERE'
      '  PROJECT_ID = :Old_PROJECT_ID'
      'FOR UPDATE NOWAIT'
    )
    SQLRefresh.Strings = (
