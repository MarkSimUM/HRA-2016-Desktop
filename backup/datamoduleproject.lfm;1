  object sqlProjectConfig_OrgInfo: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO CORE_PROJECTCONFIG'
      '  (PROJECTCONFIG_ID, PROJECT_ID, ORGANIZATION_ID, SURVEY_OPENDT, SURVEY_CLOSEDT, SURVEY_LOCKOUT_DAYS, SURVEY_URL, SURVEY_URL_ALT, SURVEY_RETURN_URL, SURVEY_WLG_HPQ, SURVEY_CUSTOMITEMS_FLG, SURVEY_CUSTOMLOGO_FILENAME, SURVEYTYPE_WEB_FLG, SURVEYTYPE_PAPER_FLG, SURVEYTYPE_CLIENTHOSTED_FLG, CSIM_METHOD, PAPER_BRE_FLG, PAPER_BULKRETURN_FLG, SURVEYLOOK_CUSTOM_FLG, SURVEYLOOK_LOGO_FILENAME, SURVEYLOOK_HMRCLOGO_HIDE_FLG, SURVEYLOOK_CUSTOMCSS_FLG, SURVEY_LANGUAGES_MULTI_FLG, REPORT_LANGUAGES_MULTI_FLG, DATAFEED_ELIGIBILITY_FLG, DATAFEED_BIOMETRIC_FLG, DATAFEED_OTHER_FLG, SURVEY_SPOUSEELIGIBLE_FLG, DATAOUT_CASE_FLG, DATAOUT_INDREPORT_FLG, DATAOUT_AGGREPORT_FLG, DATAOUT_INDCALC_FLG, SURVEY_PRIVACYSTATEMENT_FLG, RPTAGG_DATE_BEG, RPTAGG_DATE_END, RPTAGG_T1T2_FLG, RPTAGG_T1T3_FLG, RPTAGG_P1P2_FLG, RPTAGG_P1P3_FLG, RPTAGG_BENCHMARK_FLG, RPTAGG_BIOMETRIC_FLG, SURVEY_ANONYMOUS_FLG, TIMEZONE, SURVEYTEMPLATE_ID, FRAMEWORKVERSION_ID, ISACTIVE, ACTIVE_DT, SURVEY_UNITOFMEASURE, SURVEY_EXTBIOMETRICS_FLG, STATUS_ID, ISDEFAULT, PRODUCTVERSION_ID, SURVEY_SPONSORURL, REPORT_DOCTOR, SURVEY_SUBMITDAYS, SURVEY_GROUPPW, TIMESERIES_ID, PROJECT_START_DATE, PROJECT_END_DATE, ISCLOSED_FLG, DELETE_FLG, SURVEY_ID)'
      'VALUES'
      '  (:PROJECTCONFIG_ID, :PROJECT_ID, :ORGANIZATION_ID, :SURVEY_OPENDT, :SURVEY_CLOSEDT, :SURVEY_LOCKOUT_DAYS, :SURVEY_URL, :SURVEY_URL_ALT, :SURVEY_RETURN_URL, :SURVEY_WLG_HPQ, :SURVEY_CUSTOMITEMS_FLG, :SURVEY_CUSTOMLOGO_FILENAME, :SURVEYTYPE_WEB_FLG, :SURVEYTYPE_PAPER_FLG, :SURVEYTYPE_CLIENTHOSTED_FLG, :CSIM_METHOD, :PAPER_BRE_FLG, :PAPER_BULKRETURN_FLG, :SURVEYLOOK_CUSTOM_FLG, :SURVEYLOOK_LOGO_FILENAME, :SURVEYLOOK_HMRCLOGO_HIDE_FLG, :SURVEYLOOK_CUSTOMCSS_FLG, :SURVEY_LANGUAGES_MULTI_FLG, :REPORT_LANGUAGES_MULTI_FLG, :DATAFEED_ELIGIBILITY_FLG, :DATAFEED_BIOMETRIC_FLG, :DATAFEED_OTHER_FLG, :SURVEY_SPOUSEELIGIBLE_FLG, :DATAOUT_CASE_FLG, :DATAOUT_INDREPORT_FLG, :DATAOUT_AGGREPORT_FLG, :DATAOUT_INDCALC_FLG, :SURVEY_PRIVACYSTATEMENT_FLG, :RPTAGG_DATE_BEG, :RPTAGG_DATE_END, :RPTAGG_T1T2_FLG, :RPTAGG_T1T3_FLG, :RPTAGG_P1P2_FLG, :RPTAGG_P1P3_FLG, :RPTAGG_BENCHMARK_FLG, :RPTAGG_BIOMETRIC_FLG, :SURVEY_ANONYMOUS_FLG, :TIMEZONE, :SURVEYTEMPLATE_ID, :FRAMEWORKVERSION_ID, :ISACTIVE, :ACTIVE_DT, :SURVEY_UNITOFMEASURE, :SURVEY_EXTBIOMETRICS_FLG, :STATUS_ID, :ISDEFAULT, :PRODUCTVERSION_ID, :SURVEY_SPONSORURL, :REPORT_DOCTOR, :SURVEY_SUBMITDAYS, :SURVEY_GROUPPW, :TIMESERIES_ID, :PROJECT_START_DATE, :PROJECT_END_DATE, :ISCLOSED_FLG, :DELETE_FLG, :SURVEY_ID)'
    )
    SQLDelete.Strings = (
      'DELETE FROM CORE_PROJECTCONFIG'
      'WHERE'
      '  PROJECTCONFIG_ID = :Old_PROJECTCONFIG_ID'
    )
    SQLUpdate.Strings = (
      'UPDATE CORE_PROJECTCONFIG'
      'SET'
