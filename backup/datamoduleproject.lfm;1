  object dataOrg_InAccount: TOraQuery
    SQL.Strings = (
      'select '
      ''
      '  org.Account_ID '
      '  ,org.ORGANIZATION_ID'
      '  ,org.PARENT_ID '
      '  , '
      '    CASE org.ISORGGROUP '
      '     WHEN 1 THEN '
      '       ''[GROUP] '' || org.orgname '
      '     ELSE '
      '       CASE org.ISSUBGROUP'
      '        WHEN 1 then'
      '          ''[SubGroup] '' || org.orgname '
      '       ELSE'
      '       CASE org.PARENT_ID '
      '         WHEN -1 THEN'
      '             org.orgname '
      '         ELSE'
      '            org.orgname || '' ('' || org.ORGFULLNAME || '')'''
      '       END'
      '       END'
      '     END AS displayname'
      ''
      '   , org.ORGNAME'
      '  ,org.ORGFULLNAME'
      '  , org.ISORGGROUP'
      '  , org.ISSUBGROUP '
      ''
      '  FROM  core_Organization org'
      '  WHERE org.account_id = :Account_ID'
      '  start with parent_ID = -1'
      '  connect by prior organization_ID = parent_ID'
    )
    MasterSource = dsOrg_wProject
    MasterFields = 'ACCOUNT_ID'
    DetailFields = 'ACCOUNT_ID'
    Options.AutoClose = False
    Options.FieldsAsString = False
    Options.DeferredLobRead = False
    Options.CacheLobs = True
    Options.ScrollableCursor = False
    Options.FieldsOrigin = False
    Options.DefaultValues = False
    Options.RequiredFields = True
    Options.StrictUpdate = True
    Options.NumberRange = False
    Options.QueryRecCount = False
    Options.AutoPrepare = False
    Options.ReturnParams = False
    Options.TrimFixedChar = True
    Options.LongStrings = True
    Options.RemoveOnRefresh = True
    Options.FlatBuffers = False
    Options.DetailDelay = 0
    FilterOptions = []
    left = 680
    top = 88
    ParamData = <    
      item
        DataType = ftFloat
        Name = 'ACCOUNT_ID'
        ParamType = ptInput
      end>
  end
