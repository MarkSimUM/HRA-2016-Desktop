  object dataProjectConfig_Org: TOraQuery
    KeyFields = 'PROJECTCONFIG_ID'
    KeySequence = 'PROJECTCONFIG_SEQ'
    SQL.Strings = (
      'SELECT '
      '   pd.PRODUCTCODE'
      '   ,pv.VERSION'
      '   ,proj.PROJECTNAME'
      '   ,proj.STATUS_ID as projStatus_Id'
      '   ,proj.ISACTIVE As projIsActive'
      '   ,proj.PHASE as projPhase'
      '   ,proj.PRODUCTVERSION_ID '
      ' ,  projC.* '
      ' '
      ''
      '  FROM CORE_PROJECTCONFIG projC'
      '   INNER JOIN CORE_PROJECT proj'
      '     on projC.project_id = proj.project_id'
      '   '
      '   INNER JOIN GLOBAL_PRODUCTVERSION pv'
      '     ON proj.PRODUCTVERSION_ID = pv.productversion_Id'
      '   INNER JOIN GLOBAL_PRODUCT pd'
      '     ON pv.PRODUCT_ID = pd.PRODUCT_ID'
      '   '
      '  '
      '   '
      '   ORDER BY pd.PRODUCTCODE, pv.version DESC, isClosed_Flg, proj.ISACTIVE DESC, proj.projectname'
    )
    MasterSource = DM_Main.dsOrganization
    MasterFields = 'ORGANIZATION_ID'
    DetailFields = 'ORGANIZATION_ID'
    Options.AutoClose = False
    Options.FieldsAsString = False
    Options.DeferredLobRead = False
    Options.CacheLobs = True
    Options.ScrollableCursor = False
    Options.FieldsOrigin = False
    Options.DefaultValues = False
    Options.RequiredFields = True
    Options.StrictUpdate = True
    Options.NumberRange = False
    Options.QueryRecCount = False
    Options.AutoPrepare = False
    Options.ReturnParams = False
    Options.TrimFixedChar = True
    Options.LongStrings = True
    Options.RemoveOnRefresh = True
    Options.FlatBuffers = False
    Options.DetailDelay = 0
    Options.LocalMasterDetail = True
    FilterOptions = []
    left = 328
    top = 56
  end
