  object dataMasterValues: TOraQuery
    SQL.Strings = (
      'SELECT iv.VALUE,iv.DISPLAYORDER, ivL.VALUELABEL, iv.Item_Id'
      '  FROM slib_ItemValue iv'
      '  INNER JOIN slib_ItemValueLabel ivL'
      '   ON iv.itemvalue_id = ivL.itemvalue_id'
      '   AND ivL.language_id = 1'
